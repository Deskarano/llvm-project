
block main
{
    input(no[7:0], fuuuuuuu[8:0])
    output(boiiiiiiiiii, yeah[3:0])
    state(a[7:0], clk)

    when(fuuuuuuu[5:3] <= 3b111 && no == 8b10101010)
    {
        a[6] = 1b0
        a[5] = 1b1
        a[4] = 1b0

        yeah[1:0] = 2b2
    }

    when(clk == 1b1)
    {
        a[3:2] = 2b3
        a[7] = 1b1

        a[1] = 1b1 ^ a[0] + a[3]
        a[0] = 1b0
    }

    when(clk == 1b0)
    {
        boiiiiiiiiii = a[0] & clk
    }
}